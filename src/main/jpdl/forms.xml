<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE workflow-forms PUBLIC "-//OpenKM//DTD Workflow Forms 2.4//EN"
                                "http://www.openkm.com/dtd/workflow-forms-2.4.dtd">
<workflow-forms>
  <workflow-form task="avaliacao">
    <select name="options" label="Options">
      <option label="Positiva" value="Positiva"/>
      <option label="Negativa" value="Negativa"/>
      <option label="Parecer" value="Parecer"/>
    </select>
    <button name="submit" label="Submit"/>
  </workflow-form>
  <workflow-form task="avaliacao-reitoria">
    <select name="options" label="Options">
      <option label="Parecer" value="parecer"/>
      <option label="Mais Informação" value="adicionar"/>
    </select>
    <button name="submit" label="Submit"/>
  </workflow-form>
  <workflow-form task="adicionar">
    <checkbox name="adicionar" label="Adicionado"></checkbox>
    <button name="submit" label="Submit"/>
  </workflow-form>
</workflow-forms>